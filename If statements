//# C-code-1
//if statement 
#include<iostream>
using namespace std;

int main()
{
   int mark;     //int is use for integer numbers
   printf("\nenter your marks:");
   scanf("%d",&mark);
	if(mark>10)
	{
		printf("you pass the exam.");
    
	}
	else
	{
		printf("you fail the exam.");
	}
	return 0;
}
